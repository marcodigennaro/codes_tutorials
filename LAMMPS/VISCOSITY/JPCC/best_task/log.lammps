variable  vxx equal sin(0.087266462599716473902)*cos(0)
variable  vzz equal sin($(v_theta/180*PI))*sin($(v_phi/180*PI))
variable  vzz equal sin(0.087266462599716473902)*sin($(v_phi/180*PI))
variable  vzz equal sin(0.087266462599716473902)*sin(0)
variable  vyy equal cos($(v_theta/180*PI))
variable  vyy equal cos(0.087266462599716473902)
variable  xx equal v_dist*v_vxx
variable  yy equal v_dist*v_vyy
variable  zz equal v_dist*v_vzz

set atom 1 x ${xx} y ${yy} z ${zz}
set atom 1 x 0.261467228242975 y ${yy} z ${zz}
